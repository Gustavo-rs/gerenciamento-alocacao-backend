{
  "name": "gerenciamento-alocacao-backend",
  "version": "1.0.0",
  "description": "Backend API para sistema de gerenciamento de alocação de salas",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/database/seed.ts",
    "docker:db": "docker-compose up -d postgres",
    "docker:db:stop": "docker-compose stop postgres",
    "docker:pgadmin": "docker-compose up -d postgres pgadmin",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:full": "docker-compose -f docker-compose.full.yml up -d",
    "docker:full:build": "docker-compose -f docker-compose.full.yml up -d --build",
    "docker:full:down": "docker-compose -f docker-compose.full.yml down"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
